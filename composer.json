{
    "name": "simplycodedsoftware/integration-messaging-symfony",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": ["messaging", "message", "spring", "enterprise integration patterns", "symfony", "bundle", "integration"],
    "description": "Provides symfony integration to integration messaging",
    "require": {
        "php": "^7.1.3",
        "simplycodedsoftware/integration-messaging": "*",
        "simplycodedsoftware/integration-messaging-http": "*",
        "simplycodedsoftware/integration-messaging-cqrs": "*",
        "symfony/framework-bundle": "^3.0|^4.0",
        "doctrine/doctrine-bundle": "^1.8.1",
        "symfony/expression-language": "^3.0|^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "phpunit/phpunit": "^6.2.3",
        "behat/behat": "^3.3.1",
        "phan/phan": "^0.10.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },

    "autoload": {
        "psr-4": {
            "SimplyCodedSoftware\\IntegrationMessaging\\Symfony\\": "src"
        }
    },
    "repositories": [],
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/phpunit",
            "Behat\\Bootstrap\\": "tests/Behat/Bootstrap",
            "Fixture\\": "tests/Fixture"
        }
    },
    "replace": {},
    "scripts": {},
    "conflict": {},
    "extra": {
        "symfony": {
            "id": "01C56X3EZ6WW81V2TXJCPWTNSG",
            "allow-contrib": false
        }
    }
}
